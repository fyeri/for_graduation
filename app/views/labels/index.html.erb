<h1><%= t('.label_index') %></h1>

<table>
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.item.label_ids') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labels.each do |label| %>
      <tr>
        <td><%= label.name %></td>
        <td><%= link_to t('show'), label %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('.new_label'), new_label_path %>
